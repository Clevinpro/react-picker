{
  "name": "react-picker",
  "description": "Picker Component with a popup options list similiar to the picker in iOS",
  "url": "https://github.com/nickeljew/react-picker",
  "homepage": "https://github.com/nickeljew/react-picker",
  "keywords": [
    "react",
    "react-component",
    "picker",
    "chosen",
    "select",
    "options",
    "choices"
  ],
  "author": "Nickel Jianhui Zhu <nickeljew@hotmail.com>",
  "version": "1.2.12",
  "license": "MIT",
  "private": false,
  "main": "lib/picker.js",
  "style": "css/picker.css",
  "scripts": {
    "prebuild": "rm -rf lib",
    "_build": "grunt build",
    "_prepublish": "npm run build"
  },
  "babel": {
    "plugins": [
      "transform-react-jsx",
      "transform-class-properties"
    ],
    "presets": [
      "env",
      "react"
    ]
  },
  "dependencies": {
    "es6-viewpoint": "^0.1.3",
    "react-tapper": "^0.1.15"
  },
  "peerDependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "es6-docready": "^1.0.0",
    "es6-dom": "^1.0.0",
    "grunt": "^1.0.1",
    "grunt-babel": "^7.0.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-sass": "^2.0.0",
    "grunt-webpack": "^3.0.2",
    "webpack": "^3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickeljew/react-picker.git"
  }
}
